<script type="text/javascript">
window.onresize= function(){
		return moveWHLinks();
	}

var moveWHLinks = function (){
	if(document.body.offsetWidth < 769){
		if (!document.getElementById('WHlinksmoved')){
			var WHspan = document.createElement("span");
				if (document.getElementById("WHlinks")){
                    WHspan.innerHTML = document.getElementById("WHlinks").innerHTML
						.replace(/\|/gi, '')
						.replace('<span id="WHlinks_forMobile">', '<a href="/docs/searchHistory.aspx?dict=' + strDic + '">Search History</a><span id="WHlinks_onMobile">');
					WHspan.id = 'WHlinksmoved';
					document.getElementById("footer").insertBefore(WHspan, document.getElementById("footer").children[0]);
				}
			}
		}	
	};

moveWHLinks();	

//for placeholder text to fade on focus
if (document.getElementById("si")){
	document.getElementById("si").onfocus = function(){
		document.getElementById("si").placeholder = '';
	};
}
</script>

